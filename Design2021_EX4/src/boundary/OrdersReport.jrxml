	<queryString language="SQL">
		<![CDATA[SELECT tbl_Customer.PassportNumber, tbl_Customer.FirstName, tbl_Customer.LastName, Sum(IIf((TimeValue(tbl_Flight.DepartureTime)<="11:00" And TimeValue([tbl_Flight].[DepartureTime])>="05:00"),1,0)) AS morning, Sum(IIf((TimeValue(tbl_Flight.DepartureTime)<="17:00" And TimeValue(tbl_Flight.DepartureTime)>="11:00"),1,0)) AS noon, Sum(IIf((TimeValue(tbl_Flight.DepartureTime)<="23:00" And TimeValue(tbl_Flight.DepartureTime)>="17:00"),1,0)) AS evening, Sum(IIf((TimeValue(tbl_Flight.DepartureTime)<="05:00" And TimeValue(tbl_Flight.DepartureTime)>="23:00"),1,0)) AS night
FROM tbl_Flight AS tbl_Flight_1 INNER JOIN (((tbl_Flight INNER JOIN tbl_FlightTicket AS tbl_FlightTicket_1 ON tbl_Flight.FlightNum = tbl_FlightTicket_1.FlightNum) INNER JOIN tbl_Customer ON tbl_FlightTicket_1.CustomerPassportNumber = tbl_Customer.PassportNumber) INNER JOIN tbl_FlightTicket ON tbl_Customer.PassportNumber = tbl_FlightTicket.CustomerPassportNumber) ON tbl_Flight_1.FlightNum = tbl_FlightTicket.FlightNum
GROUP BY tbl_Customer.PassportNumber, tbl_Customer.FirstName, tbl_Customer.LastName, tbl_Flight_1.updateDate
HAVING (((tbl_Flight_1.updateDate)=Date()));
]]>
